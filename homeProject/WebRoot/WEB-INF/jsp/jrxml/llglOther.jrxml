	<queryString>
		<![CDATA[select  case when tc.RealAddress is not null then tc.RealAddress else tc.Address end Address,tc.Descr custDescr,tc.DocumentNo,tc.Mobile1,te1.NameChi designManDescr,tia.ProjectMan projectManDescr,tx1.NOTE typeDescr,tia.Phone projectManPhone,tx2.NOTE sendTypeDescr,tc.Area,
                isnull(tspl.Descr,'') supplCodeDescr,case when tc.custtype='2' then te5.nameChi else te4.nameChi end softDesignManDescr,tia.Remarks,ltrim(rtrim(tia.itemType1)) itemType1,te3.NameChi AppName,case when tc.custtype='2' then te5.Phone else te4.Phone end  softDesignManPhone,
                case when tct.IsAddAllInfo = '0' then tc.Mobile1 else '***********' end CustPhone,tit1.Descr itemType1Descr,tia.type,
                isnull((select b.LogoFile  from tCustomer a  left join tCusttype b on a.CustType=b.Code where b.LogoFile is not null and a.Code=tia.custCode ),'') LogoFile,isnull(twh.IsManagePosi,'0') IsManagePosi,tia.Date,tia.status
                from tItemApp tia
                left join tCustomer tc on tia.CustCode = tc.Code
                left join tEmployee te1 on te1.Number = tc.DesignMan
                left join tEmployee te2 on te2.Number = tc.ProjectMan
                left join tXTDM tx1 on tx1.ID='ITEMAPPTYPE' and tx1.CBM = tia.Type
                left join tXTDM tx2 on tx2.ID='DELIVTYPE' and tx2.CBM=tia.DelivType
                left join tSupplier tspl on tspl.Code = tia.SupplCode
                left join tEmployee te3 on tia.AppCZY=te3.Number
                left join tCustStakeholder tcsh on tia.CustCode = tcsh.CustCode and tcsh.Role='50'
                left join tEmployee te4 on te4.Number = tcsh.EmpCode
 	    left join tEmployee te5 on te5.Number = tc.DesignMan
                left join tCusttype tct on tct.Code = tc.CustType
                left join tItemType1 tit1 on tit1.Code = tia.ItemType1
                left join tWareHouse twh on tia.WHCode=twh.Code
                where no=$P{appNo}]]>
	</queryString>
