	<queryString>
		<![CDATA[select a.address,a.Area,a.MaterialFee,
                case when b.CustCode is not null then a.BaseFee_Dirct*b.ContainBase else a.BaseFee_Dirct end BaseFee_Dirct,
                case when b.CustCode is not null then b.BaseDiscPer*b.ContainBase*100 else 100 end BaseDiscPer,
                case when b.CustCode is not null then round(b.BaseDiscPer*a.BaseFee_Dirct*b.ContainBase,0)
                else a.BaseFee_Dirct*100 end BaseDiscPerFee,
                case when b.CustCode is not null then a.BaseFee_Comp*b.ContainBase else a.BaseFee_Comp end BaseFee_Comp,
                case when b.CustCode is not null then (a.ManageFee_Base*b.ContainBase+a.ManageFee_soft*b.ContainSoft+
                +a.ManageFee_Main*b.ContainMain+a.ManageFee_Serv*b.ContainMainServ
                +a.ManageFee_Int*b.ContainInt+a.ManageFee_Cup*b.ContainCup )*b.ContainBase else a.ManageFee end ManageFee,
                case when b.CustCode is not null then b.BaseDisc1*b.ContainBase else 0 end BaseDisc1,
                case when b.CustCode is not null then b.BaseDisc2*b.ContainBase else 0 end BaseDisc2,
                case when b.CustCode is not null then a.MainFee*b.ContainMain else a.MainFee end MainFee,
                case when b.CustCode is not null then a.IntegrateFee*b.ContainInt else a.IntegrateFee end IntegrateFee,
                case when b.CustCode is not null then a.CupboardFee*b.ContainCup else a.CupboardFee end CupboardFee,
                case when b.CustCode is not null then a.MainDisc*b.ContainMain+a.CupBoardDisc*b.ContainCup+a.IntegrateDisc*b.ContainInt
                else a.MainDisc+a.CupBoardDisc+a.IntegrateDisc end MainDisc,
                case when b.CustCode is not null then a.SoftFee*b.ContainSoft else a.SoftFee end SoftFee,
                case when b.CustCode is not null then a.SoftDisc*b.ContainSoft else a.SoftDisc end SoftDisc,
                case when b.CustCode is not null then a.SoftOther*b.ContainSoft else a.SoftOther end SoftOther,
                case when b.CustCode is not null then a.MainServFee*b.ContainMainServ else a.MainServFee end MainServFee,
                c1.BasePer*100 BasePer1,c2.BasePer*100 BasePer2,c3.BasePer*100 BasePer3,c4.BasePer*100 BasePer4,
                c1.BasePay BasePay1,c2.BasePay BasePay2,c3.BasePay BasePay3,c4.BasePay BasePay4,
                c1.ItemPer*100 ItemPer1,c2.ItemPer*100 ItemPer2,c3.ItemPer*100 ItemPer3,c4.ItemPer*100 ItemPer4,
                c1.ItemPay ItemPay1,c2.ItemPay ItemPay2,c3.ItemPay ItemPay3,c4.ItemPay ItemPay4,
                c1.DesignFee DesignFee1,c2.DesignFee DesignFee2,c3.DesignFee DesignFee3,c4.DesignFee DesignFee4,
                c1.PrePay PrePay1,c2.PrePay PrePay2,c3.PrePay PrePay3,c4.PrePay PrePay4,b.PayType,
	    case when c2.CustCode is not null then '1' else '0' end showFlag2,
                case when c3.CustCode is not null then '1' else '0' end showFlag3,
                case when c4.CustCode is not null then '1' else '0' end showFlag4,
                prd1.PayRemark  PayRemark1 ,prd2.PayRemark  PayRemark2,prd3.PayRemark  PayRemark3,
                prd4.PayRemark  PayRemark4,pr.DesignFeeType,isnull(prd1.IsRcvDesignFee,0) IsRcvDesignFee1,
                isnull(prd2.IsRcvDesignFee,0) IsRcvDesignFee2,isnull(prd3.IsRcvDesignFee,0) IsRcvDesignFee3,isnull(prd4.IsRcvDesignFee,0) IsRcvDesignFee44,isnull(b.ReturnDesignFee,0) ReturnDesignFee,cast(isnull(b.position,0)as money) DesignFeeSd,
case when prd1.IsRcvDesignFee='1' or prd2.IsRcvDesignFee='1'  or prd3.IsRcvDesignFee='1'  or prd4.IsRcvDesignFee='1' then '1' else '0' end  IsRcvDesignFee
                from tCustomer a
                left JOIN dbo.tCustProfit b ON a.Code=b.CustCode
                left JOIN dbo.tCustPayPre c1 ON c1.CustCode=a.Code AND c1.PayType='1' and c1.BasePer+c1.ItemPer+c1.DesignFee+c1.PrePay+c1.BasePay+c1.ItemPay<>0
                left JOIN dbo.tCustPayPre c2 ON c2.CustCode=a.Code AND c2.PayType='2' and c2.BasePer+c1.ItemPer+c2.DesignFee+c2.PrePay+c2.BasePay+c2.ItemPay<>0
                left JOIN dbo.tCustPayPre c3 ON c3.CustCode=a.Code AND c3.PayType='3' and c3.BasePer+c3.ItemPer+c3.DesignFee+c3.PrePay+c3.BasePay+c3.ItemPay<>0
                left JOIN dbo.tCustPayPre c4 ON c4.CustCode=a.Code AND c4.PayType='4' and c4.BasePer+c4.ItemPer+c4.DesignFee+c4.PrePay+c4.BasePay+c4.ItemPay<>0
				left join  tPayRule pr on pr.CustType=a.CustType and pr.PayType=b.PayType
				left outer join tPayRuleDetail prd1 on prd1.no=pr.No and prd1.paynum='1'
                left outer join tPayRuleDetail prd2 on prd2.no=pr.No and prd2.paynum='2'
                left outer join tPayRuleDetail prd3 on prd3.no=pr.No and prd3.paynum='3'
                left outer join tPayRuleDetail prd4 on prd4.no=pr.No and prd4.paynum='4'
                where a.Code=$P{code}]]>
	</queryString>
