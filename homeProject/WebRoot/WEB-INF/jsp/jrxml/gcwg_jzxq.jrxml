	<queryString>
		<![CDATA[declare @CustCode nvarchar(20)=$P{CustCode}
select c.Descr FixAreaDescr,b.Descr BaseItemDescr,d.Descr Uom,a.Qty,
a.UnitPrice,a.Material,a.LineAmount,a.Remark,tc.Address,tc.Area
from tcustomer tc
left join tBaseItemReq a on a.CustCode=tc.Code and a.qty <> 0
left outer join tBaseItem b on b.Code=a.BaseItemCode
left outer join tFixArea c on c.Pk=a.FixAreaPK
left outer join tUom d on b.Uom=d.Code
where a.CustCode=@CustCode
order by c.DispSeq, a.DispSeq]]>
	</queryString>
