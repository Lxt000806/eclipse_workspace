	<queryString>
		<![CDATA[select bip.PK,bip.CustCode,bip.FixAreaPK,bip.BaseItemCode,bip.Qty,bip.Cost,
UnitPrice,bip.LineAmount,bip.Remark,bip.DispSeq,bip.Material,bip.IsCheck,
bi.Descr BaseItemCodeDescr,u.Descr uomDescr,fa.Descr FixAreaDescr
from tBaseItemPlan bip
left join tBaseItem bi on bi.Code=bip.BaseItemCode
left join tUOM u on u.Code=bi.Uom
left join tFixArea fa on fa.PK=bip.FixAreaPK
left join tCustProfit cp on cp.CustCode=bip.CustCode
where bip.CustCode=$P{code}
and bi.Category<>'2' and  bip.IsOutSet='1' and cp.ContainBase=1 and not exists(select 1 from tCommiBasePersonal in_a where in_a.BaseItemType1=bi.BaseItemType1)
 order by  fa.dispSeq,bip.DispSeq]]>
	</queryString>
