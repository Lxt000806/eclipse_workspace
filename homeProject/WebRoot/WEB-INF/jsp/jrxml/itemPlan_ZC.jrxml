	<queryString>
		<![CDATA[select    *
                      from      (   select    round(case when ct.TaxExpr is null or ct.TaxExpr = '' then 0 else
                                            dbo.fGetExp( replace( replace( replace(
                                                            ct.TaxExpr, '@DesignFee@', i.DesignFee),
                                                    '@MainSetFee@',	i.MainSetFee ),
                                            '@ContractFee@', i.MainFee-MainDisc+ManageFee_Main )
                                        )  end, 0) taxFee
                                    ,a.*, c.Descr FixAreaDescr,  b.Descr ItemDescr, b.CommiType,  b.isfixprice, d.Descr IntProdDescr,
                                            f.Descr Uom,  e.Descr ItemType1Descr,  g.Descr ItemType2Descr, sql.Descr SqlCodeDescr, st.Descr ItemSetDescr,
                                            xt.Note isoutsetdescr,  k.Descr ItemType3Descr, b.Model,  b.SizeDesc, i.Address, i.Area,
                                            i.MainDisc, i.code, convert(varchar(100), i.CrtDate, 112) CrtDate,  i.CustType, isnull(cp.MainItemRem,'') MainItemRem,
                                            ( select    count(*) markupNum
                                              from      dbo.tItemPlan tip
                                                        left join tCustomer tc on tip.CustCode = tc.Code
                                              where     rtrim(tip.CustCode) = $P{code} and rtrim(tip.ItemType1) = 'ZC' and tip.IsService = 0 and tc.ContainMain = '1' and tip.Markup <> 100
                                            ) markupNum,
                                            case when a.LineAmount <> 0
                                                 then round(round(a.LineAmount - a.Qty * a.Cost - a.ProcessCost,
                                                              0) / a.LineAmount,
                                                            4) * 100
                                                 else 0
                                            end ProfitPer,
                                            case when cct.CmpnyName <> '' and cct.CmpnyName is not null
                                                 then cct.CmpnyName else '有家装饰' end CmpnyName, i.MainFee,
                                            i.ContainMain, i.ManageFee_Main,te.NameChi DesignManDescr
                                  from      ( select    a.PK, a.CustCode, a.FixAreaPK,  a.IntProdPK,  a.ItemCode, a.iscommi,  a.itemSetNo,
                                                        a.ItemType1, a.ProjectQty, a.Qty,  a.UnitPrice,  a.ProcessCost, a.Cost,  a.BefLineAmount,
                                                        a.Markup,  ( a.LineAmount - isnull(a.ProcessCost,  0) ) TmpLineAmount,
                                                        a.LineAmount,  a.DispSeq, a.Remark, a.LastUpdate, a.LastUpdatedBy, a.ActionLog, a.Expired,
                                                        a.Cost * a.Qty CostAll, a.isoutset, tct.isPartDecorate,i.DesignMan
                                              from      tItemPlan a
                                                        left join dbo.tCustomer i on i.Code = a.CustCode
                                                        left join tCusttype tct on tct.Code = i.CustType
                                              where     a.CustCode = $P{code} and a.ItemType1 = 'ZC' and a.isService = 0 and i.ContainMain = '1'
                                            ) a
                                            left join tItem b on b.Code = a.ItemCode
                                            left join tBrand sql on sql.code = b.sqlcode
                                            left join tItemType1 e on e.Code = a.ItemType1
                                            left join tItemType2 g on g.Code = b.ItemType2
                                            left join tFixArea c on c.Pk = a.FixAreaPK
                                            left join tIntProd d on d.PK = a.IntProdPK
                                            left join tUom f on b.Uom = f.Code
                                            left join tItemSet st on a.itemSetNo = st.no
                                            left join txtdm xt on a.IsOutSet = xt.Cbm and xt.id = 'YESNO'
                                            left  join tItemType3 k on k.Code = b.ItemType3
                                            left join dbo.tCustomer i on i.Code = a.CustCode
                                            left join tCustType ct on ct.code = i.custtype
                                            left join tBuilder bd on bd.code = i.BuilderCode
                                            left join tRegion r on r.code = bd.regionCode
                                            left join tCmpCustType cct on cct.CustType = ct.Code and cct.CmpCode = r.CmpCode
                                            left join tCompany cp on cp.Code = r.CmpCode
                                            left join tEmployee te on a.DesignMan = te.Number
                                ) a
                      order by  a.DispSeq, a.FixAreaPk]]>
	</queryString>
