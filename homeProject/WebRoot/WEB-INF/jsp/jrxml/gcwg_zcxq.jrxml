	<queryString>
		<![CDATA[declare @CustCode nvarchar(20)=$P{CustCode}
select  a.IsService, a.DispSeq, a.CustCode, c1.Address, c1.Area, a.FixAreaPK, a.IntProdPK, a.ItemCode, a.ItemType1,
a.Qty, a.SendQty, f.Descr Uom, a.UnitPrice, a.MarkUp mMarkUp,b.SizeDesc,h.Descr ItemType3Descr,b.Model,i.Descr SqlDescr,
case when a.MarkUp <> 100 then ltrim(str(a.MarkUp)) + '%' else '-'
end MarkUp, round(a.BefLineAmount * a.MarkUp / 100, 0) TmpLineAmount, a.ProcessCost, a.LineAmount, a.Remark,
c.Descr FixAreaDescr, b.Descr ItemDescr, d.Descr IntProdDescr, e.Descr ItemType1Descr, g.Code Item2Code,
right('00000' + cast(g.DispSeq as varchar(20)), 4) + ' ' + g.Descr ItemType2Seq, c1.SoftPlanRemark
from tCustomer c1
left outer join tItemReq a on c1.Code = a.CustCode and a.ItemType1='ZC' and (a.qty<>0 or a.ProcessCost<>0 or a.SendQty<>0)
left outer join tItem b on b.Code = a.ItemCode
left outer join tItemType1 e on e.Code = a.ItemType1
left outer join tItemType2 g on g.Code = b.ItemType2
left outer join tFixArea c on c.Pk = a.FixAreaPK
left outer join tIntProd d on d.PK = a.IntProdPK
left outer join tUom f on b.Uom = f.Code
left outer join tBrand i on i.Code=b.SqlCode
left outer join tItemType3 h on h.Code=b.ItemType3
where a.CustCode=@CustCode
order by  g.DispSeq, a.DispSeq]]>
	</queryString>
