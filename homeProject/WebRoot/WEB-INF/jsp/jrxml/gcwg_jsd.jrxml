	</parameter>
	<queryString>
		<![CDATA[declare @custCode nvarchar(20)=$P{CustCode}
select f.qz CmpnyName,g.QZ CmpnyAddress,h.qz CmpnyPhone,i.QZ CmpnyFax,j.QZ JSRemark,
a.Code,a.Descr,a.Address,a.DesignMan,a.BusinessMan,a.ChgBaseFee,
case when c.Type='1' and b.ContractFeeRepType in('2','3','4') then a.StdDesignFee else a.DesignFee end DesignFee,
a.ContractFee-ReturnDesignFee*case when c.Type='1' and b.ContractFeeRepType in('2','3','4') then 1 else 0 end ContractFee,
a.Tax,a.FirstPay,a.SecondPay,a.ThirdPay,a.FourPay,
(a.ContractFee-ReturnDesignFee
	*case when c.Type='1' and b.ContractFeeRepType in('2','3','4') then 1 else 0 end
	+case when c.Type='1' and b.ContractFeeRepType in('2','3','4') then a.StdDesignFee else a.DesignFee end
	+a.Tax) as ContractSum,
convert(varchar(100), a.CrtDate, 112) CrtDate,a.CustType,a.SpecialDisc,a.DocumentNo,
(select 1 from tItemChg where status='1' and custcode=@custCode
      union select 1 from tBaseItemChg where status='1' and custcode=@custCode
      union select 1 from tConFeeChg where status='OPEN' and custcode=@custCode) fzs,
d.FinRcvCzy,e.NameChi FinRcvCzyName
from tCustomer a
left join tPayRule b on a.PayType=b.PayType and a.CustType=b.CustType
left join tCusttype c on a.CustType=c.Code
left join tCustCheck d on a.Code=d.CustCode
left join tEmployee e on d.FinRcvCzy=e.Number
left join tXTCS f on f.id='CmpnyName'
left join tXTCS g on g.id='CmpnyAddress'
left join tXTCS h on h.id='CmpnyPhone'
left join tXTCS i on i.id='CmpnyFax'
left join tXTCS j on j.id='JSRemark'
where a.Code=@custCode]]>
	</queryString>
