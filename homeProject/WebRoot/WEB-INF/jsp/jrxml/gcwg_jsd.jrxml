	<subDataset name="dsCheck" uuid="5e9ded7b-25df-4163-9c29-1b9d4c48338a">
		<parameter name="CustCode" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[declare @custCode nvarchar(20)=$P{CustCode}
select isnull(( select top 1 b.NameChi ZcComfirmEmpDescr from tItemCheck a
left join tEmployee b on b.Number=a.ConfirmEmp where CustCode=@custCode and a.ItemType1='ZC' ),'') ZcComfirmEmpDescr,
isnull(( select top 1 b.NameChi ZcComfirmEmpDescr from tItemCheck a
left join tEmployee b on b.Number=a.ConfirmEmp where CustCode=@custCode and a.ItemType1='RZ' ),'') RzComfirmEmpDescr,
isnull(( select top 1 b.NameChi ZcComfirmEmpDescr from tItemCheck a
left join tEmployee b on b.Number=a.ConfirmEmp where CustCode=@custCode and a.ItemType1='JC' ),'') JcComfirmEmpDescr]]>
		</queryString>
		<field name="ZcComfirmEmpDescr" class="java.lang.String"/>
		<field name="RzComfirmEmpDescr" class="java.lang.String"/>
		<field name="JcComfirmEmpDescr" class="java.lang.String"/>
	</subDataset>
	<subDataset name="dsJsje" uuid="bff5ef40-e853-4115-a9de-92503c5183ee">
		<parameter name="CustCode" class="java.lang.String">
			<defaultValueExpression><![CDATA[$P{CustCode}]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[declare @custCode nvarchar(20)=$P{CustCode}
select sum(isnull(Amount,0)) Amount from(
select sum(isnull(Amount,0)) Amount
FROM tItemChg
WHERE Status <> '3' AND CustCode= @custCode union all
select sum(isnull(Amount,0)) Amount
FROM tBaseItemChg
WHERE Status <> '3' AND CustCode= @custCode union all
select sum(isnull(ChgAmount,0)) Amount
from tConFeeChg
where Status in ( 'CONFIRMED', 'OPEN' )
and ChgType in ('1','2','4') and CustCode = @custCode union all
select (ContractFee+DesignFee+Tax) Amount from tCustomer
where Code = @custCode
) a]]>
		</queryString>
		<field name="Amount" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="dsJswk" uuid="40b5cf5c-af48-498f-a5a8-ba8eec740b5d">
		<parameter name="CustCode" class="java.lang.String">
			<defaultValueExpression><![CDATA[$P{CustCode}]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[declare @custCode nvarchar(20)=$P{CustCode}
select sum(isnull(Amount,0))-sum(isnull(PayAmount,0))-sum(isnull(SpecialDis,0)) Amount from(
select sum(isnull(Amount,0)) Amount,0 SpecialDis,0 PayAmount
FROM tItemChg
WHERE Status <> '3' AND CustCode= @custCode union all
select sum(isnull(Amount,0)) Amount,0 SpecialDis,0 PayAmount
FROM tBaseItemChg
WHERE Status <> '3' AND CustCode= @custCode union all
select sum(isnull(ChgAmount,0)) Amount,0 SpecialDis,0 PayAmount
from tConFeeChg
where Status in ( 'CONFIRMED', 'OPEN' )
and ChgType in ('1','2','4') and CustCode = @custCode union all
select (ContractFee+DesignFee+Tax) Amount,SpecialDisc,0 PayAmount from tCustomer
where Code = @custCode union all
select 0 Amount,0 SpecialDiscc,sum(isnull(Amount,0)) payAmount from tCustPay where CustCode =@custCode
) a]]>
		</queryString>
		<field name="Amount" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="LogoFile" class="java.lang.String" isForPrompting="false">
