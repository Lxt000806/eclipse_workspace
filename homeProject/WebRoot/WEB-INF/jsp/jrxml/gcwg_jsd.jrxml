	<subDataset name="dsZx" uuid="34fc6f2e-4ae3-486e-a896-2659881004a7">
		<parameter name="CustCode" class="java.lang.String"/>
		<queryString>
			<![CDATA[declare @custCode nvarchar(20)=$P{CustCode}
if  object_id('tempdb..#temp5') is not NULL drop table #temp5
SELECT identity(int,1,1) as SerNO,ss.Date,ss.ItemTypeDescr,ss.Amount  into #temp5 FROM
((SELECT CONVERT(varchar(12) , date, 111 ) AS Date,
case when i.IsService=0 then it.Descr else it.Descr+'服务产品' end +
case when i.Status='1' then '-申请' else '' end as ItemTypeDescr,Amount
FROM tItemChg i      LEFT OUTER JOIN tItemType1 it ON  it.code = i.ItemType1
WHERE Amount >0  AND Status <> '3' AND CustCode= @custCode)
UNION ALL (SELECT CONVERT(varchar(12) , date, 111 ) AS Date,
case when Status='1' then '基础-申请' else '基础' end  as ItemTypeDescr ,Amount FROM tBaseItemChg
WHERE Amount >0 AND Status <> '3' AND CustCode= @custCode)
UNION all (select convert(varchar(12), cfc.date,111) as Date,
case when cfc.status = 'OPEN' then x1.Note+'-申请' else x1.note end as ItemTypeDescr,
cfc.ChgAmount from tConFeeChg  cfc
left outer join tXTDM x1 on x1.id='CHGTYPE' and x1.CBM=cfc.ChgType
where cfc.ChgAmount>0 and cfc.Status in ('CONFIRMED','OPEN')
and cfc.ChgType in ('1','2')  AND cfc.CustCode= @custCode)
union all ( select  convert(varchar(12), cfcsj.date, 111) as Date,
case when cfcsj.status = 'OPEN' then x1.Note + '-申请'  else x1.note end as ItemTypeDescr, cfcsj.ChgAmount
from    tConFeeChg cfcsj      left outer join tXTDM x1 on x1.id = 'CHGTYPE'  and x1.CBM = cfcsj.ChgType
where   cfcsj.ChgAmount > 0      and cfcsj.Status in ( 'CONFIRMED', 'OPEN' )     and cfcsj.ChgType ='4'
and cfcsj.CustCode = @custCode )    ) ss ORDER BY Date ASC
SELECT SerNO,Date,ItemTypeDescr,Amount,(SELECT sum(Amount) FROM #temp5) AllAmount FROM #temp5 order by date]]>
		</queryString>
		<field name="SerNO" class="java.lang.Integer"/>
		<field name="Date" class="java.lang.String"/>
		<field name="ItemTypeDescr" class="java.lang.String"/>
		<field name="Amount" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="AllAmount" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="dsJx" uuid="425e32d5-b7d1-42b0-9f8a-b70c4ce79cb8">
		<parameter name="CustCode" class="java.lang.String"/>
		<queryString>
			<![CDATA[declare @custCode nvarchar(20)=$P{CustCode}
if  object_id('tempdb..#temp6') is not NULL drop table #temp6
SELECT identity(int,1,1) as SeNO ,ss.Date,ss.ItemTypeDesc,ss.Amount  into #temp6 FROM
((SELECT CONVERT(varchar(12) , date, 111 ) AS Date,
case when i.IsService=0 then it.Descr else it.Descr+'服务产品' end +
case when i.Status='1' then '-申请' else '' end  ItemTypeDesc,Amount
FROM tItemChg i      LEFT OUTER JOIN tItemType1 it ON  it.code = i.ItemType1
WHERE Amount <0  AND Status <> '3' AND CustCode= @custCode)
UNION ALL (SELECT CONVERT(varchar(12) , date, 111 ) AS Date,
case when Status='1' then '基础-申请' else '基础' end as ItemTypeDesc ,Amount FROM tBaseItemChg
WHERE Amount <0 AND Status <> '3' AND CustCode= @custCode)
UNION all (select convert(varchar(12), cfc.date,111) as Date,
case when cfc.status = 'OPEN' then x1.Note+'-申请' else x1.note end as ItemTypeDescr,
cfc.ChgAmount from tConFeeChg  cfc
left outer join tXTDM x1 on x1.id='CHGTYPE' and x1.CBM=cfc.ChgType
where cfc.ChgAmount<0 and cfc.Status in ('CONFIRMED','OPEN')
and cfc.ChgType in ('1','2') AND cfc.CustCode= @custCode)
union all ( select  convert(varchar(12), cfcsj.date, 111) as Date,
case when cfcsj.status = 'OPEN' then x1.Note + '-申请'  else x1.note end as ItemTypeDescr, cfcsj.ChgAmount
from    tConFeeChg cfcsj
left outer join tXTDM x1 on x1.id = 'CHGTYPE'  and x1.CBM = cfcsj.ChgType
where   cfcsj.ChgAmount < 0      and cfcsj.Status in ( 'CONFIRMED', 'OPEN' )
and cfcsj.ChgType ='4'       and cfcsj.CustCode = @custCode )    ) ss ORDER BY Date ASC
SELECT SeNO,Date,ItemTypeDesc,(0-Amount) as Amount,(SELECT sum(0-Amount) FROM #temp6) AllAmount FROM #temp6 order by Date]]>
		</queryString>
		<field name="SeNO" class="java.lang.Integer"/>
		<field name="Date" class="java.lang.String"/>
		<field name="ItemTypeDesc" class="java.lang.String"/>
		<field name="Amount" class="java.math.BigDecimal"/>
		<field name="AllAmount" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="dsKhfk" uuid="afe7a2ab-14e4-4c97-bab0-2d7641a2d22b">
		<parameter name="CustCode" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[declare @custCode nvarchar(20)=$P{CustCode}
select CONVERT(varchar(12),Date,111) AS Date,isnull(Amount,0) Amount,Remarks,
(select sum(isnull(Amount,0)) from tCustPay where CustCode =@custCode) AllAmount from tCustPay where CustCode =@custCode]]>
