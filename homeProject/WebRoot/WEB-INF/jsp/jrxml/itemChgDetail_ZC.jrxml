	<queryString>
		<![CDATA[select a.IsService,a.IsCommi,a.PK,a.No,a.ReqPK,a.FixAreaPK,a.IntProdPK,c.ItemType2,
b.Descr  FixAreaDescr,c.Descr itemDescr,c.Uom,d.Descr uomDescr,c.isFixPrice, isnull(e.Qty,0) PreQty, f.Descr IntProdDescr,g.Descr ItemType2Descr,a.ItemCode,a.Qty,a.Cost,a.UnitPrice,a.BefLineAmount, Convert(decimal(18,2),a.Markup) Markup,round(a.Qty*a.UnitPrice*a.Markup/100,0) TmpLineAmount,isnull(a.ProcessCost,0) ProcessCost,a.LineAmount,a.Remarks,a.LastUpdate,a.LastUpdatedBy,a.ActionLog,a.Expired,a.DispSeq,a.DiscCost,a.IsOutSet,case when a.IsOutSet=0 then '否' else '是' end isoutsetdescr,h.Remarks remark,h.Date,i.Address,i.DocumentNo,i.Area,c.Model,h.Amount,h.BefAmount,h.DiscAmount,c.SizeDesc,c.MarketPrice,
j.Descr SqlDescr,k.Descr ItemType3Descr,H.ManageFee,l.Type,CASE WHEN  m.CmpnyName IS NULL OR m.CmpnyName='' THEN '有家装饰' ELSE  m.CmpnyName END  CmpnyName ,i.code,CONVERT(varchar(100), i.CrtDate, 112) CrtDate,i.CustType,(SELECT  COUNT(*) markupNum FROM  dbo.tItemChgDetail  WHERE No=$P{No} AND Markup<>100 ) markupNum,H.Tax
from tItemChgDetail a
LEFT JOIN tFixArea b ON a.FixAreaPK=b.PK
LEFT JOIN titem c ON a.ItemCode=c.Code
LEFT JOIN tUOM d ON c.Uom=d.Code
LEFT join tItemReq e on e.PK=a.ReqPK
LEFT join tIntProd f on f.Pk=a.IntProdPK
LEFT join tItemType2 g on g.Code=c.ItemType2
LEFT  JOIN  tItemChg h ON h.No=a.No
LEFT  JOIN  tCustomer i ON h.CustCode=i.Code
LEFT  JOIN tBrand j ON c.SqlCode=j.Code
LEFT  JOIN tItemType3 k ON k.Code=c.ItemType3
LEFT JOIN tCusttype l ON l.Code=i.CustType
left join tBuilder bd on bd.code=i.builderCode
left join tRegion rg on rg.code =bd.regionCode
left join tCmpCustType m on m.CmpCode = rg.CmpCode and m.CustType =i.custType and m.expired='F'
 where a.No= $P{No}  order by a.DispSeq]]>
	</queryString>
