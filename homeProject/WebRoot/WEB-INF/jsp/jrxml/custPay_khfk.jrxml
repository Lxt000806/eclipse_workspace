	<queryString>
		<![CDATA[SELECT top 1 isnull(o.namechi,'') designLeader,isnull(l.desc2,'') posiDescr,(select d.Desc2 cmpDescr  from tCustomer a
left join tBuilder b on b.code=a.BuilderCode
left join tRegion c on c.Code=b.RegionCode
left join tCompany d on d.Code=c.CmpCode
where a.code=$P{custCode}) cmpDescr ,a.remarks,a.Contractday,a.BaseDisc,a.MarketFund,a.gift,a.documentNo,a.Mobile1,a.stdDesignFee,a.returnDesignFee,a.code,a.descr,a.address,a.contractFee,a.designFee,a.firstPay,a.secondPay,a.thirdPay,a.fourPay,
a.BaseFee_Dirct,a.BaseFee_Comp,a.ContainBase,a.area,b.NOTE StatusDescr ,c.NOTE LayOutDescr ,a.DiscRemark,a.PayRemark,
a.CupBoardDisc * a.ContainCup + a.IntegrateDisc * a.ContainInt + a.SoftDisc
* a.ContainSoft + a.MainDisc * a.ContainMain + a.BaseDisc * a.ContainBase SumDisc ,
a.FirstPay + SecondPay + ThirdPay + FourPay SumPay ,
a.ContractFee + a.DesignFee ContractDesign ,
a.BaseFee_Dirct * a.ContainBase BaseFee_DirctContainBase ,
a.BaseFee_Comp * a.ContainBase BaseFee_CompContainBase ,
a.ManageFee * a.ContainBase ManageFeeContainBase ,
a.MainFee * a.ContainMain MainFeeContainMain ,
a.SoftFee * a.ContainSoft SoftFeeContainSoft ,
a.IntegrateFee * a.ContainInt IntegrateFeeContainInt ,
a.CupboardFee * a.ContainCup CupboardFeeContainCup ,
a.MainServFee * a.ContainMainServ MainServFeeContainMainServ ,
ISNULL(CONVERT(CHAR(10), a.signdate, 120), '') sSignDate ,
CAST(dbo.fGetEmpNameChi(a.Code, '00') + ' ' + dbo.fGetDept2Descr(a.Code, '00') + ' ' + dbo.fGetDept1Descr(a.Code, '00') AS NVARCHAR(1000)) DesignManDescr ,
CAST(dbo.fGetEmpNameChi(a.Code, '01') + ' ' + dbo.fGetDept2Descr(a.Code, '01') AS NVARCHAR(1000)) BusinessManDescr ,
 isnull(( select sum(LineAmount) BaseFee0240
         from   tBaseItemPlan b
                inner join txtcs xc on xc.id = 'LongFeeCode' and ',' + xc.QZ + ',' like '%,' + b.BaseItemCode + ',%'
         where  b.custcode = a.code
       ), 0) BaseFee0240,isnull(d.zcAmount, 0) zcAmount,isnull(e.jcAmount, 0) jcAmount,isnull(f.rzAmount, 0) rzAmount,isnull(g.baseAmount, 0) baseAmount,
 isnull(a.firstPay,0)+isnull(a.secondPay,0)+isnull(a.thirdPay,0)+isnull(a.fourPay,0)+isnull(case when tpr.DesignFeeType='2' then a.StdDesignFee else a.DesignFee end,0) wdz ,
 ct.IsPubReturn,isnull(ct.ReturnAmount,0) ReturnAmount, a.Tax,a.MainSetFee,a.SetMinus,a.SetAdd,e1.NameChi AgainManDescr,ct2.type isTc,
 h.AgainSignCount,ct2.type CustTypeType,ct2.IsPartDecorate,
 isnull(case when a.BaseFee_Dirct=0 or a.BaseDisc-37*a.Area<0 then 0 else (a.BaseDisc-37*a.Area-isnull(nm.NoCalMangeFee,0))/a.BaseFee_Dirct end,0)  DirctFeeDiscount,
 case when ga.Record=1 or cg.Record=1 then '1' else '0' end  hasGift,
 case when ct2.type='1' and ct2.IsPartDecorate='0' then '5' else '4' end TitleNum
FROM    tCustomer a
LEFT OUTER JOIN tXTDM b ON a.Status = b.CBM
                           AND b.ID = 'CUSTOMERSTATUS'
LEFT OUTER JOIN tXTDM c ON a.LayOut = c.CBM
                           AND c.ID = 'LAYOUT'
 left join (select CustCode,sum(Amount) zcAmount from tItemChg where Expired='F' and Status='1' and ItemType1='ZC' group by CustCode) d on d.CustCode = a.Code
 left join (select CustCode,sum(Amount) jcAmount from tItemChg where Expired='F' and Status='1' and ItemType1='JC' group by CustCode) e on e.CustCode = a.Code
 left join (select CustCode,sum(Amount) rzAmount from tItemChg where Expired='F' and Status='1' and ItemType1='RZ' group by CustCode) f on f.CustCode = a.Code
 left join (select sum(Amount) baseAmount,CustCode from tBaseItemChg where Expired='F' and Status='1' group by CustCode) g on g.CustCode = a.Code
 left join tPayRule tpr on tpr.CustType=a.Custtype and tpr.PayType=a.PayType
 left join tCustTax ct on ct.CustCode = a.Code
                                 left join tEmployee e1 on a.AgainMan=e1.Number
 left join tCustType ct2 on a.custtype=ct2.code
left join (
	select count(1) AgainSignCount,CustCode
	from tAgainSign in_a
	group by in_a.CustCode
) h on h.CustCode=a.Code
left join tCustStakeholder j on j.CustCode = a.Code and j.Role ='01'
left join tEmployee k on k.Number = j.EmpCode
left join tPosition l on l.Code=k.Position
left join tCustStakeholder m on m.CustCode = a.Code and m.Role ='00'
left join tEmployee n on n.Number =m.EmpCode
left join tEmployee o on o.Department2 = n.Department2 and o.IsLead ='1' and o.Status <> 'sus'
left join (
		select sum(a.LineAmount) NoCalMangeFee, a.CustCode
		from tBaseItemPlan a
        left outer join tBaseItem b on b.Code = a.BaseItemCode
        where b.IsCalMangeFee = '0' group by a.CustCode
    ) nm on a.Code=nm.CustCode
left join(select 1 Record from tGiftAppDetail a left join tgiftapp b on a.no = b.no where custcode=$P{custCode} and b.status in('send', 'return'))ga on 1=1
left join(select 1 Record from tcustgift a where custcode =$P{custCode})cg on 1=1
WHERE   a.code =$P{custCode}]]>
	</queryString>
