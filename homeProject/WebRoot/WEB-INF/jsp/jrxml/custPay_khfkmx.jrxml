	<parameter name="custCode" class="java.lang.String"/>
	<queryString>
		<![CDATA[select a.date,a.amount,a.remarks,isnull(b.DocumentNo,'') DocumentNo,c.NOTE type  from tcustpay a
left join tPayCheckOut b on  b.No=a.PayCheckOutNo
left join tXTDM c on c.CBM=a.Type and c.id='CPTRANTYPE'
 where custcode=$P{custCode}]]>
