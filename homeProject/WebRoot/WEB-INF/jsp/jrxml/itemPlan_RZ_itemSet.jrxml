	<queryString>
		<![CDATA[select  a.PK, a.CustCode, a.FixAreaPK, a.IntProdPK, a.ItemCode,
                        a.ItemType1, a.ProjectQty, a.Qty, f.Descr Uom, a.Cost,
                        case when cct.CmpnyName <> '' and cct.CmpnyName is not null
                             then cct.CmpnyName
                             else '有家装饰'
                        end CmpnyName, a.UnitPrice, a.BefLineAmount,
                        a.MarkUp Markup,
                        round(a.BefLineAmount * a.MarkUp / 100, 0) TmpLineAmount,
                        isnull(a.ProcessCost, 0) ProcessCost, a.LineAmount,
                        a.DispSeq, a.Remark, a.LastUpdate, a.LastUpdatedBy,
                        a.ActionLog, a.Expired, c.Descr FixAreaDescr,
                        b.Descr ItemDescr, d.Descr IntProdDescr, d.IsCupboard,
                        case when d.isCupboard = 1 then '是'
                             else '否'
                        end isCupboardDescr, b.commitype,
                        e.Descr ItemType1Descr, g.Code Item2Code,
                        case when a.ItemSetNo is null or a.ItemSetNo = ''
                             then g.Descr
                             else h.Descr
                        end ItemType2Descr, b.IsFixPrice,
                        rtrim(b.ItemType2) ItemType2, a.IsOutSet,
                        x1.NOTE IsOutSetDescr, b.ItemType3,
                        it3.Descr ItemType3Descr, a.ItemSetNo,
                        h.descr itemsetDescr, i.Address, i.Area, i.code,
                        i.SoftOther,i.PlanSendDate_soft PlanSendDate,
                        convert(varchar(100), i.CrtDate, 112) CrtDate,
                        i.CustType, i.SoftPlanRemark, i.SoftDisc,
                        ( select    count(*) markupNum
                          from      dbo.tItemPlan tip
                                    left join tCustomer tc on tc.Code = tip.CustCode
                          where     rtrim(tip.CustCode) = $P{code} and rtrim(tip.ItemType1) = 'RZ' and tip.IsService = 0 and tc.ContainSoft = '1' and tip.Markup <> 100 and ( tip.ItemSetNo is null or tip.ItemSetNo = '' )
                        ) markupNum, b.SizeDesc, b.MarketPrice, i.Custtype,
                        i.SoftFee, i.ContainSoft, i.ManageFee_Soft,
                        case when a.ItemSetNo is not null then h.Descr
                             else g.Descr
                        end groupByFlag,
                        case when  isnull(dt.amount,0)=0 then 0
						else  case when (i.SoftFee-i.SoftDisc)*i.ContainSoft+i.ManageFee_Soft>isnull(dt.amount,0)
							  then isnull(dt.amount,0) else (i.SoftFee-i.SoftDisc)*i.ContainSoft+i.ManageFee_Soft end
						end DiscTokenAmount,te.NameChi DesignManDescr
                from    tItemPlan a
                        left outer join tItem b on b.Code = a.ItemCode
                        left outer join tItemType1 e on e.Code = a.ItemType1
                        left outer join tItemType2 g on g.Code = b.ItemType2
                        left outer join tFixArea c on c.Pk = a.FixAreaPK
                        left outer join tIntProd d on d.PK = a.IntProdPK
                        left outer join tUom f on b.Uom = f.Code
                        left outer join tXTDM x1 on x1.ID = 'YESNO' and x1.CBM = a.IsOutSet
                        left outer join tItemType3 it3 on it3.Code = b.ItemType3
                        left join titemset h on h.no = a.itemsetno
                        left join dbo.tCustomer i on i.Code = a.CustCode
                        left join tCusttype ct on ct.Code = i.CustType
		left join tEmployee te on i.DesignMan = te.Number
                        left join tBuilder bd on bd.code = i.BuilderCode
                        left join tRegion r on r.code = bd.regionCode
                        left join tCmpCustType cct on cct.CustType = ct.Code and cct.CmpCode = r.CmpCode
                        left join (select sum(Amount) Amount,UseCustCode from tDiscToken
					     where Status in('3','4') and UseStep='1' group by UseCustCode
						)dt on dt.UseCustCode=a.CustCode
                where   rtrim(a.CustCode) = $P{code} and rtrim(a.ItemType1) = 'RZ' and a.IsService = 0 and i.ContainSoft = '1'
                order by a.ItemSetNo desc,
                        case when a.ItemSetNo is not null then h.Descr
                             else g.Descr
                        end, a.DispSeq]]>
	</queryString>
